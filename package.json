{
    "name": "vscode-obx",
    "displayName": "vscode-obx",
    "description": "Syntax highlighting for OBX language",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.43.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "obx",
            "aliases": ["OBX", "obx"],
            "extensions": [".obx"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "obx",
            "scopeName": "source.obx",
            "path": "./syntaxes/obx.tmLanguage.json"
        }],
        "snippets": [{
            "language": "obx",
            "path": "./snippets.json"
        }]
    }
}
